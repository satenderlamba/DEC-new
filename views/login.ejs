<!doctype html>
 <html>
 <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link href="styles.css" rel="stylesheet">
    <title>DEC- Login</title>
 
    <script src="https://www.gstatic.com/firebasejs/6.1.0/firebase-app.js"></script>
    <!-- Add Firebase products that you want to use -->
    <script  src="https://www.gstatic.com/firebasejs/6.1.0/firebase-auth.js"></script>
    <script  src="https://www.gstatic.com/firebasejs/6.1.0/firebase-firestore.js"></script>
    <script src="https://www.gstatic.com/firebasejs/6.1.0/firebase-database.js"></script>

</head>
<body>
    <body class="full-height-grow">
        <div class="container full-height-grow">
          <header class="main-header">
            <a href="index.html" class="brand-logo">
              <img src="Ecell-LOGO-01.png">
              <div class="brand-logo-name">DEC</div>
            </a>
            <nav class="main-nav">
              <ul>
                <li><a href="index.html">Home</a></li>
                <li><a href="index.html">Events</a></li>
                <li><a href="index.html">Login</a></li>
              </ul>
            </nav>
          </header>
          <section class="login-main-section">
            <h1 class="login-text">
              Log
              <span class="accent-text">in.</span>
            </h1>
            <form class="login-form">
             <div class="input-group email">
                <label>Email or Phone:</label>
                <input type="email" id="txtemail" value="" class="form-control">
              </div>
              <div class="input-group">
                <label>Password:</label>
                <input type="password" id="txtpassword" value="" class="form-control">
              </div>
              <div class="input-group">
                <button type="submit" id="loginButton" onclick="signInUsers()"class="btn btn-warning">Login</button>
              </div>
              <div class="input-group-last">
                  <a class="forgot-button"><label>Forgot Password?</label></a>
              </div>

              <div><button type="submit" id="logoutButton" onclick="logout()"class="btn btn- 
                warning">logout</button>   </div>
            </form>
          </section>
        </div>
        
    <script src="/js/firebase.js"></script>
   </body>
  </html>